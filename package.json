{
  "name": "hugo-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV=production hugo",
    "prebuild": "NODE_ENV=production webpack -p",
    "serve": "concurrently --kill-others \"hugo server --disableFastRender\" \"webpack --watch\"",
    "preserve": "webpack",
    "clean": "rm -rf ./dist && rm -rf ./public"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "dependencies": {
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donnyburnside/hugo-boilerplate.git"
  },
  "author": "Donny Burnside <donnyburnside1@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/donnyburnside/hugo-boilerplate#readme"
}
